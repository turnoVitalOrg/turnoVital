  "tableData": "{{\n  (chkEspecialidades.selectedOptionValues || []).map(especId => {\n    // buscamos la especialidad\n    const espec = getEspecialidades.data.find(e => e.id == especId);\n\n    // buscamos TODOS los turnos cuya especialidad es esa\n    const total = getTurnos.data.filter(turnoItem => {\n      const profesional = getProfesionales.data.find(p => p.id == turnoItem.profesional_id);\n      return profesional?.especialidad_id == especId;\n    }).length;\n\n    return {\n      especialidad: espec ? espec.nombre : \"Sin especialidad\",\n      totalTurnos: total\n    };\n  })\n}}\n",
